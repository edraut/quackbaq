<%= javascript_include_tag "jquery-1.4.4.min.js" %>
<script>
 jQuery.noConflict();
</script>
<%= javascript_include_tag "application.js" %>
<%= javascript_include_tag "ajax_bindings.js" %>
<%= javascript_include_tag "ui_bindings.js" %>
<%= javascript_include_tag "jquery-ui-1.8.7.custom.min.js" if @jquery_ui%>
<%= javascript_tag do %>
	var authenticity_token = '<%= form_authenticity_token %>';
	var preload_date = new Date();
	<% unless session[:time_zone_offset] %>
		document.location.href = '<%= user_session_url %>?time_zone_offset='+(preload_date.getTimezoneOffset()/60);
	<% end %>
<% end %>
